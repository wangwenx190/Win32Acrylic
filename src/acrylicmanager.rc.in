#include <Windows.h>
#include "resource.h"

IDI_DEFAULTICON ICON "@CMAKE_CURRENT_SOURCE_DIR@/assets/defaulticon.ico"
IDI_DEFAULTICONSM ICON "@CMAKE_CURRENT_SOURCE_DIR@/assets/defaulticonsm.ico"

#ifndef ACRYLICMANAGER_STATIC

VS_VERSION_INFO VERSIONINFO
FILEVERSION     @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
PRODUCTVERSION  @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
FILEFLAGSMASK   0x3fL
#ifdef _DEBUG
FILEFLAGS       VS_FF_DEBUG
#else
FILEFLAGS       0x0L
#endif
FILEOS          VOS_NT_WINDOWS32
FILETYPE        VFT_DLL
FILESUBTYPE     VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName",      "wangwenx190\0"
            VALUE "FileDescription",  "AcrylicManager\0"
            VALUE "FileVersion",      "@PROJECT_VERSION@\0"
            VALUE "LegalCopyright",   "MIT License\0"
            #ifdef _DEBUG
            VALUE "OriginalFilename", "AcrylicManagerd.dll\0"
            #else
            VALUE "OriginalFilename", "AcrylicManager.dll\0"
            #endif
            VALUE "ProductName",      "AcrylicManager\0"
            VALUE "ProductVersion",   "@PROJECT_VERSION@\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

#endif
